[Config] ProjectSettings {
    compiler: {
        version: "1.0.0",
        optimization: true,
        debug: false
    },
    
    selectors: {
        auto_add_class: true,
        auto_add_id: false,
        disable_script_auto_add_class: true
    },
    
    modules: {
        load_strategy: "parallel",
        cache_enabled: true
    }
}

[Template] ConfiguredTemplate {
    div {
        style {
            /* 使用&引用选择器 */
            & {
                background: blue;
            }
        }
    }
}
