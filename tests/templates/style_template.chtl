// 测试样式模板
[Template] @Style button-primary {
    background: #007bff;
    color: white;
    padding: 10px 20px;
    border: none;
    border-radius: 4px;
    cursor: pointer;
}

[Template] @Style button-secondary {
    background: #6c757d;
    color: white;
    padding: 10px 20px;
    border: none;
    border-radius: 4px;
    cursor: pointer;
}

div {
    button {
        style {
            @Style button-primary;
        }
        "Primary Button"
    }
    
    button {
        style {
            @Style button-secondary;
            margin-left: 10px;
        }
        "Secondary Button"
    }
}